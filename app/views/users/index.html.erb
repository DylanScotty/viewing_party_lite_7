<div style="text-align: center;">
<h1>Viewing Party Lite</h1>
<% if logged_in? %>
  <%= link_to "Log Out", logout_path, data: { turbo_method: :delete } %>
<% else %>
  <%= button_to "Sign In", login_path, method: :get %>
  <%= button_to "Register", register_path, method: :get %>
<% end %>
</div>



